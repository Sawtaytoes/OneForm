    "dev:cypress": "cypress open",
    "dev:storybook": "start-storybook --port 6006 --ci",
    "dev:test": "jest --cache --onlyChanged --errorOnDeprecated --watch",
    "start": "yarn && yarn dev"
  },
  "browser": "dist/oneform.umd.min.js",
  "main": "dist/oneform.cjs.js",
